{
  "version": 2,
  "builds": [
    {
      "src": "output/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/output/main.js",
      "methods":[
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ],
      "headers":{
        "Access-Control-Allow-Origin":"*"
      }
    }
  ],
  "outputDirectory":"output"
}

